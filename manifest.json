{
  "name": "Comment tu t'appelles déjà ?",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Mémorisez les prénoms des theodoers",
  "permissions": [
    "tabs",
    "http://www.theodo.fr/fr/theodo/les-theodoers",
    "alarms",
    "storage",
    "notifications"
  ],
  "background": {
    "persistent": false,
    "scripts": ["backgroundScript.js"]
  },
  "browser_action": {
    "default_title": "Comment tu t'appelles déjà ?",
    "default_icon": "icons/ic_person_pin_circle_black_18dp_2x.png"
  }
}
